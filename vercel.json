{
  "builds": [
    {
      "src": "vitabi/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vitabi/wsgi.py"
    }
  ],
  "env": {
    "GITHUB_ACCESS_TOKEN": "@github_access_token"
  }
}
